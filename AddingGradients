#include "udf.h"

Define_EXECUTE_AT_END(Jvec)
{
	Domain *d
	Thread *t;
	cell_t c;
	
	thread_loop_c (t,d)
	  {
			begin_c_loop (c,t)
				{ 
						C_UDMI(c,t,0) = NV_MAG(C_YI_G(c,t,0))
						C_UDMI(c,t,1) = NV_MAG(C_YI_G(c,t,1))
						C_UDMI(c,t,2) = NV_MAG(C_YI_G(c,t,2))
						C_UDMI(c,t,3) = NV_MAG(C_YI_G(c,t,3))
						C_UDMI(c,t,4) = NV_MAG(C_YI_G(c,t,4))
				}
		}
	
	
